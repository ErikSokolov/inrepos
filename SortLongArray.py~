foo=400
import random

a = []

i=0
for i in range(random.randint(1,10)):
    a.append(random.randint(1,10))
print(a)
long = len(a) 
print(long)

i=0
ii = 0
while a[ii] >= a[i]:
    swap = a[0]
    swip = a[i]
    a[i] = swap
    a[0] = swip
    i = i + 1
    if i > long:
        ii = ii + 1
        i = 0
print(a)

    

